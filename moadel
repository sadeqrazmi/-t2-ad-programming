  cout<<"be barname moadel daneshjooha   khosh amadid!"<<endl; 
 
 cout<<"tedade daneshjooha ra vared konid"<<endl;
 cin>>a;
 	for(int i=1;i<a+1;i++){
 		
 		cout<<"nomre daneshjoo "<<i<<" ra vred konid"<<endl;
 		cin>>b;
 		if(b>20){
 		cout<<"nomre balaye 20!!!"<<endl;
 		cout<<"dobare vared konid"<<endl;
 		cin>>b;
	
       	}
 		else{
 		c=b+c;
 		
 
	 }
	 
	    if(b>max)
     	max=b;
 	
       	
		if(b<min)
		min=b;
		
	        }
 		
	
	 cout<<"moadel in daneshjooha dar dars barname nvisi "<<c/a<< " hast"<<"   kamtarin nomre: "<<min<<"  bishtarin nomre: "<<max;
 }
